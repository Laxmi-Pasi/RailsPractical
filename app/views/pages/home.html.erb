
<% if current_myuser %>
<p><%= current_myuser.email %>! </p>
<p><%= current_myuser.role %></p>
<%= button_to "sign Out",destroy_myuser_session_path,method: :delete %>    
<% else %>
<p> You are not signed in. </p>
<%= link_to "Sign Up!", new_myuser_registration_path %> <br/>
<%= link_to "login", new_myuser_session_path %>
<% end %>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>